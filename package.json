{
  "name": "@blex41/word-search",
  "version": "1.0.10",
  "description": "Word search puzzle generator",
  "main": "src/index.js",
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "scripts": {
    "build": "webpack --mode=production",
    "build:doc": "jsdoc ./src -d docs",
    "test": "jest --verbose --coverage",
    "lint": "eslint --ext .js --max-warnings=0 ./src",
    "format": "eslint ./src --fix",
    "test:watch": "jest --verbose --coverage --watch",
    "coverage": "jest --coverage --coverageReporters=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blex41/word-search.git"
  },
  "keywords": [
    "wordsearch",
    "word",
    "search",
    "puzzle",
    "mishmash",
    "grid",
    "generator"
  ],
  "author": "blex41",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blex41/word-search/issues"
  },
  "homepage": "https://github.com/blex41/word-search#readme",
  "dependencies": {
    "diacritics": "^1.3.0",
    "lodash": "^4.17.11"
  },
  "jest": {
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "babel-loader": "^8.0.4",
    "coveralls": "^3.0.2",
    "eslint": "^5.12.0",
    "eslint-loader": "^2.1.1",
    "jest": "^23.6.0",
    "jsdoc": "^3.5.5",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2"
  }
}
